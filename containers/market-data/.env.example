# Market Data Service Environment Variables
# Copy this to .env and add your actual API credentials

# Kraken API (for BTC/USD and other crypto pairs)
KRAKEN_API_KEY=
KRAKEN_API_SECRET=

# Binance API (for additional crypto data)
BINANCE_API_KEY=
BINANCE_API_SECRET=

# Database connection
DATABASE_URL=postgresql://signalcartel:signalcartel@database:5432/signalcartel

# Redis connection
REDIS_URL=redis://redis-cache:6379

# Service configuration
NODE_ENV=production
LOG_LEVEL=info
FETCH_INTERVAL=30000